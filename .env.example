### Example environment variables (DO NOT COMMIT SECRETS)

# Postgres (used by container initialization in compose)
POSTGRES_DB=app_db
POSTGRES_USER=app_user
POSTGRES_PASSWORD=app_password

# user-service
USER_DB_HOST=postgres
USER_DB_PORT=5432
USER_DB_USER=user_service
USER_DB_PASSWORD=password
USER_DB_NAME=users_db
USER_PORT=3001

# order-service
ORDER_DB_HOST=postgres
ORDER_DB_PORT=5432
ORDER_DB_USER=order_service
ORDER_DB_PASSWORD=password
ORDER_DB_NAME=orders_db
ORDER_PORT=3002
